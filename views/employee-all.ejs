<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
</head>
<body>
    <h1><%= title %></h1>
    <a href="/">Home</a><br>
    <form action="/employees/new" method="POST">
        <label for="SSN">Employee ID:</label>
        <input type="text" id="SSN" name="SSN" required>
        
        <label for="DOB">DOB:</label>
        <input type="date" id="DOB" name="DOB" required>
        
        <label for="Fname">First Name:</label>
        <input type="text" id="Fname" name="Fname" required>
        
        <label for="Minit">M.I.:</label>
        <input type="text" id="Minit" name="Minit">
        
        <label for="Lname">Last Name:</label>
        <input type="text" id="Lname" name="Lname" required>
        
        <label for="address">Address:</label>
        <input type="text" id="address" name="address">
        
        <input type="submit" value="Create Employee">
    </form>
    <table>
        <tr>
            <th>Employee ID</th>
            <th>DOB</th>
            <th>First Name</th>
            <th>M.I.</th>
            <th>Last Name</th>
            <th>Address</th>
        </tr>
        <% for(let i=0; i < employees.length; i++) { %>
            <tr>
                <td><a href="id/:<%= employees[i].SSN %>"><%= employees[i].SSN %></a></td>
                <td><%= employees[i].DOB %></td>
                <td><%= employees[i].Fname %></td>
                <td><%= employees[i].Minit %></td>
                <td><%= employees[i].Lname %></td>
                <td><%= employees[i].address %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>